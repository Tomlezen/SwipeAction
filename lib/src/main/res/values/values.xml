<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string name="app_name">lib</string>

    <item name="swipe_key_gravity" type="id" />
    <item name="swipe_key_offset_helper" type="id" />

    <declare-styleable name="SwipeLayout">
        <attr name="swipe_orientation" format="enum">
            <enum name="horizontal" value="1" />
            <enum name="vertical" value="0" />
        </attr>
        <attr name="swipe_mode" format="enum">
            <enum name="drawer" value="0" />
            <enum name="smooth" value="1" />
            <enum name="parallax" value="2" />
        </attr>
        <attr name="swipe_parallax_multiplier" format="float" />
        <attr name="swipe_enable" format="boolean" />
        <attr name="swipe_behavior" format="string" />
        <attr name="swipe_sensitivity" format="float" />
        <attr name="swipe_layout_gravity" format="enum">
            <enum name="start" value="8388611" />
            <enum name="end" value="8388613" />
        </attr>
        <attr name="swipe_direction" format="enum">
            <enum name="any" value="2" />
            <enum name="endToStart" value="1" />
            <enum name="startToEnd" value="2" />
        </attr>
        <attr name="swipe_start_max_distance" format="enum|dimension">
            <enum name="auto" value="-1" />
        </attr>
        <attr name="swipe_end_max_distance" format="enum|dimension">
            <enum name="auto" value="-1" />
        </attr>
    </declare-styleable>

</resources>